  keywords = {wichtig-gelesen},
  file = {C:\DATA\Syncthing\Zotero_Attachments\Rauschenbach et al_2016_Mass Spectrometry as a Preparative Tool for the Surface Science of Large.pdf}
}

@article{rinke_active_2014,
  title = {Active {{Conformation Control}} of {{Unfolded Proteins}} by {{Hyperthermal Collision}} with a {{Metal Surface}}},
  author = {Rinke, Gordon and Rauschenbach, Stephan and Harnau, Ludger and Albarghash, Alyazan and Pauly, Matthias and Kern, Klaus},
